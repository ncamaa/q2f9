{
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.codeActionsOnSave": ["source.fixAll.eslint"],
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "vue"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#d98ed7",
    "activityBar.background": "#d98ed7",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#eeefcf",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#d98ed7",
    "statusBar.background": "#cc68ca",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#bf42bd",
    "statusBarItem.remoteBackground": "#cc68ca",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#cc68ca",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#cc68ca99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "cSpell.words": ["Pinia"],
  "editor.formatOnSaveMode": "file",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.requireConfig": true,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "peacock.color": "#cc68ca"
}
